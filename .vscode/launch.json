{
	"version": "0.1.0",
	"configurations": [
		{
			"name": "OpenOCD Debug",
			"type": "gdb",
			"request": "attach",
			"printCalls": true,
			"executable": "${workspaceRoot}/build/simplesamplemqtt.elf",
			"target": ":3333",
			"remote": true,
			"cwd": "${workspaceRoot}",
			"autorun": [
				"break app_main"
			],
			"gdbpath": "/home/user/xtensa-esp32-elf/bin/xtensa-esp32-elf-gdb"
		}
	]
}